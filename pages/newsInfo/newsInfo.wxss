/**newsInfo.wxss**/

.content_main {
  width: 750rpx;
  height: 200rpx;
  border-top: 2rpx solid #f5f5f5;
  border-bottom: 2rpx solid #f5f5f5;
}
.content_main_top {
  width: 710rpx;
  line-height: 50rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
  color: #0c499c;
  font-size: 32rpx;
  overflow: hidden;
  font-weight: bold;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp :2;
  -webkit-box-orient: vertical;
}

.content_main_list {
  width: 710rpx;
  height: 40rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
  color: #737373;
  display: flex;
  justify-content: space-between;
}
.content_main_list_item {
  width: auto;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
}
.content_main2 {
  width: 710rpx;
  height: auto;
  margin-left: 20rpx;
  margin-top: 40rpx;
}
.content_main2_file {
  width: 750rpx;
  line-height: 35rpx;
  text-align: center;
  color: #1989fa;
  margin-bottom: 40rpx;
}