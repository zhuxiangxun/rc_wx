/**index.wxss**/
@import "../../templates/indexHeader/indexHeader.wxss";

/**青岛人才创业平台地图**/
.map_bg {
  width: 750rpx;
  height: 460rpx;
  border-top: 15rpx solid #f5f5f5;
}
.map_content {
  width: 710rpx;
  height: 420rpx;
  margin: 20rpx 20rpx;
  background-color: #f5f5f5;
  border-radius: 8rpx;
}
.map_content_title {
  width: 670rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  margin: 0 20rpx;
}
.map_content_main {
  width: 670rpx;
  height: 350rpx;
  margin: 0 20rpx;
}
.map_content_main_list {
  width: 360rpx;
  height: 300rpx;
  float: left;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.map_content_main_list_item {
  width: 110rpx;
  height: 130rpx;
  margin-top: 10rpx;
  text-align: center;
}
.map_content_main_list_item_title {
  width: 110rpx;
  line-height: 70rpx;
  font-size: 28rpx;
}
.map_content_main_img {
  width: 300rpx;
  height: 300rpx;
  float: right;
}
/**外部链接**/
.link {
  width: 750rpx;
  height: 350rpx;
  border-top: 15rpx solid #f5f5f5;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.link:after{
  content:'';
  width: 150rpx;
}
.link_item {
  width: 150rpx;
  height: 130rpx;
  margin-top: 20rpx;
  text-align: center;
}
.link_item_title {
  width: 150rpx;
  line-height: 70rpx;
  font-size: 28rpx;
}
/**信息快讯**/
.news {
  width: 750rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  text-align: center;
  border-top: 15rpx solid #f5f5f5;
  border-bottom: 15rpx solid #f5f5f5;
  margin-bottom: 100rpx;
  z-index: 999;
}