page {
  overflow: hidden;
}
/**map.wxss**/
.content {
  width: 750rpx;
  /* height: calc(100vh - 220rpx); */
  height: 100vh;
}
.content_map {
  width: 100%;
  height: 100vh;
}
/**默认底部**/
.content_footer {
  width: 750rpx;
  height: 320rpx;
  position: absolute;
  bottom: 0rpx;
  background-color: #FFF;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  z-index: 1;
}
.content_footer_address {
  width: 200rpx;
  height: 40rpx;
  margin-top: 40rpx;
  margin-left: 40rpx;
}
.content_footer_address_icon {
  width: 40rpx;
  height: 40rpx;
  float: left;
}
.content_footer_address_title {
  width: 150rpx;
  line-height: 40rpx;
  float: left;
  margin-left: 10rpx;
  font-size: 28rpx;
}

.content_footer_list {
  width: 670rpx;
  height: 200rpx;
  margin-left: 40rpx;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.content_footer_list_item {
  width: 100rpx;
  height: 200rpx;
  margin-top: 10rpx;
  text-align: center;
}
.content_footer_list_item_img {
  width: 80rpx;
  height: 80rpx;
  margin-left: 10rpx;
  margin-top: 10rpx;
}
.content_footer_list_item_title {
  width: 100rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  color: #535353;
}


/**点击坐标显示单条数据**/
.content_info {
  width: 750rpx;
  height: 200rpx;
  position: absolute;
  bottom: 0rpx;
  background-color: #FFF;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  z-index:1;
}
.content_info_left {
  width: 600rpx;
  height: 120rpx;
  margin-top: 40rpx;
  margin-left: 20rpx;
  float: left;
}
.content_info_left_img {
  width: 120rpx;
  height: 120rpx;
  float: left;
}
.content_info_left_info {
  width: 450rpx;
  height: 120rpx;
  float: right;
}
.content_info_left_info_title {
  width: 450rpx;
  line-height: 60rpx;
  color: #0c499c;
  font-weight: bold;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp :1;
  -webkit-box-orient: vertical;
}
.content_info_left_info_adress {
  width: 580rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #535353;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp :1;
  -webkit-box-orient: vertical;
}
.content_info_right {
  width: 80rpx;
  height: 220rpx;
  float: right;
  margin-right: 20rpx;
}
.content_info_right_close {
  width: 80rpx;
  height: 60rpx;
  margin-top: 10rpx;
}
.content_info_right_address {
  width: 80rpx;
  height: 90rpx;
}
.content_info_right_address_img {
  width: 80rpx;
  height: 60rpx;
}
.content_info_right_address_font {
  width: 80rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #535353;
  text-align: center;
}

/**搜索结果**/
.search_content {
  width: 750rpx;
  height: 220rpx;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  background-color: #FFF;
  position: absolute;
  bottom: 0;
  z-index: 1;
}
.search_content_left {
  width: 600rpx;
  height: 220rpx;
  margin-left: 20rpx;
  float: left;
}
.search_content_left_btn {
  width: 80rpx;
  height: 60rpx;
  margin-left: 335rpx;
}
.search_content_left_address {
  width: 200rpx;
  height: 40rpx;
}
.search_content_left_address_icon {
  width: 40rpx;
  height: 40rpx;
  float: left;
}
.search_content_left_address_title {
  width: 150rpx;
  line-height: 40rpx;
  float: left;
  margin-left: 10rpx;
  font-size: 28rpx;
}
.search_content_left_num {
  width: 600rpx;
  line-height: 80rpx;
  color: #000;
  font-size: 28rpx;
  clear: both;
}
.search_content_right {
  width: 80rpx;
  height: 220rpx;
  float: right;
}
.search_content_right_close {
  width: 80rpx;
  height: 60rpx;
  margin-top: 10rpx;
}
.search_content_right_btn {
  width: 80rpx;
  height: 60rpx;
  margin-top: 40rpx;
}

/**搜索列表**/
.searchInfo_content {
  width: 750rpx;
  height: auto;
  background-color: #FFF;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  position: absolute;
  bottom: 0;
  z-index: 1;
}
.searchInfo_content_top {
  width: 80rpx;
  height: 60rpx;
  margin-left: 335rpx;
}
.searchInfo_content_input {
  width: 710rpx;
  height: 100rpx;
  margin-left: 20rpx;
}
.van-icon {
  color: #f58b1a;
}
.van-search {
  padding: 0px!important;
  height: 100%;
}
.van-search__content {
  background-color: #eeeeee!important;
}
.searchInfo_content_btn_list {
  width: 710rpx;
  height: 50rpx;
  margin: 10rpx 20rpx 20rpx 20rpx;
}
.searchInfo_content_btn_list_item {
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  text-align: center;
  color: #000;
  background-color: #eeeeee;
  float: left;
  border-radius: 50rpx;
  margin-right: 10rpx;
}
.searchInfo_content_btn_list_item_hover {
  background-color: #0c499c;
  color: #FFF;
}
.searchInfo_content_menu {
  width: 710rpx;
  height: auto;
}

