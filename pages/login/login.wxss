/**login.wxss**/
.login {
  width: 750rpx;
  height: 100vh;
  background-color: #f4f5f7;
  position: relative;
}
.login_bg {
  width: 750rpx;
  height: 440rpx;
  overflow: hidden;
  background-image: url('https://cim.qdcsdn.cn/rc_wx/login_03.png');
}
.login_avatar {
  width: 210rpx;
  height: 210rpx;
  border-radius: 210rpx;
  margin-top: 80rpx;
  margin-left: 270rpx;
}
.login_avatar_title {
  width: 750rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  margin-top: 10rpx;
  color: #FFF;
  text-align: center;
}
.login_info {
  width: 670rpx;
  height: 340rpx;
  background-color: #FFF;
  border-radius: 20rpx;
  position: absolute;
  left: 40rpx;
  top: 380rpx;
}
.login_info_list {
  width: 610rpx;
  height: 200rpx;
  margin-left: 30rpx;
  margin-top: 60rpx;
}
.van-cell {
  margin-top: 10rpx;
  background-color: #f4f5f7!important;
}
.login_info_btn {
  position: absolute;
  width: 350rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  background-color: #0c499c;
  text-align: center;
  left: 200rpx;
  bottom: 150rpx;
  color: #FFF;
  font-size: 32rpx;
}